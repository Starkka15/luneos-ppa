[Unit]
Description=Luna Service Hub Daemon (webOS IPC)
After=dbus.service
Wants=dbus.service
Before=luna-surfacemanager.service db8.service

[Service]
Type=simple
ExecStart=/usr/sbin/ls-hubd --conf /etc/luna-service2/ls-hubd.conf
Restart=on-failure
RestartSec=1

[Install]
WantedBy=multi-user.target
